{
  "name": "queertoronto",
  "version": "1.0.0",
  "description": "A Single Page Web Application that presents a community website with a list of events and organizations",
  "main": "server.js",
  "scripts": {
    "start": "npm run prod",
    "dev": "concurrently --kill-others \"webpack-dev-server --inline --hot\" \"nodemon ./server.js\"",
    "createDatabase": "node ./server/model/initial/createDatabase.js",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --mode=production",
    "prod": "node ./server.js",
    "checklint": "eslint --ignore-path .gitignore .",
    "fixlint": "./node_modules/.bin/eslint --fix --ignore-path .gitignore .",
    "makepretty": "./node_modules/.bin/prettier --write --ignore-path .gitignore '**/*.js' '**/*.vue'",
    "test": "jest"
  },
  "keywords": [
    "Community",
    "Queer",
    "Toronto",
    "Gay",
    "Lesbian",
    "Bisexual",
    "Transgendered",
    "Trans",
    "Events"
  ],
  "homepage": "https://github.com/tomrembrown/QTCommunity",
  "author": "Tom Brown <tom@tomrembrown.com>",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/tomrembrown/QTCommunity.git"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "concurrently": "^4.1.0",
    "connect-history-api-fallback": "^1.6.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "jest": "^24.7.1",
    "luxon": "^1.12.1",
    "md5": "^2.2.1",
    "microdata-node": "^1.0.0",
    "password-hash": "^1.2.2",
    "pg": "^7.10.0",
    "prettier": "^1.17.0",
    "vue": "^2.6.10",
    "vue-datetime": "^1.0.0-beta.10",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.0",
    "weekstart": "^1.0.0",
    "xmldom": "^0.1.27",
    "xpath": "0.0.27"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "compression-webpack-plugin": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^3.0.1",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.11",
    "sass-loader": "^7.1.0",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  }
}
